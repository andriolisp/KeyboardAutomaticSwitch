# Copy to ~/.config/kb-autoswitch/config.env and edit

# External keyboard name as shown in /proc/bus/input/devices
EXTERNAL_NAME="MX KEYS B"

# Laptop keyboard name
INTERNAL_NAME="AT Translated Set 2 keyboard"

# Layouts (XKB)
EXTERNAL_LAYOUT="us+intl"
INTERNAL_LAYOUT="gb+intl"

# Seconds between allowed switches
SWITCH_COOLDOWN="0.25"
